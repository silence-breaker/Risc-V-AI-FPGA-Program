    %store/vec4 v0x564dc0e7f3f0_0, 0, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x564dc0e7f590_0, 0, 2;
    %pushi/vec4 5, 0, 16;
    %store/vec4 v0x564dc0e7f310_0, 0, 16;
    %delay 10, 0;
    %load/vec4 v0x564dc0e7f490_0;
    %cmpi/e 15, 0, 16;
    %jmp/0xz  T_1.0, 6;
    %vpi_call/w 4 26 "$display", "[PASS] Case 1: 10 * 1 + 5 = 15" {0 0 0};
    %jmp T_1.1;
T_1.0 ;
    %vpi_call/w 4 27 "$error", "[FAIL] Case 1 Expected 15, Got %d", v0x564dc0e7f490_0 {0 0 0};
T_1.1 ;
    %pushi/vec4 20, 0, 8;
    %store/vec4 v0x564dc0e7f3f0_0, 0, 8;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x564dc0e7f590_0, 0, 2;
    %pushi/vec4 100, 0, 16;
    %store/vec4 v0x564dc0e7f310_0, 0, 16;
    %delay 10, 0;
    %load/vec4 v0x564dc0e7f490_0;
    %cmpi/e 80, 0, 16;
    %jmp/0xz  T_1.2, 4;
