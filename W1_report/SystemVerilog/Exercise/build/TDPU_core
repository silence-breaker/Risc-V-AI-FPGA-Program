v0x564dc0e7f160_0 .var "o_next_sum", 15 0;
E_0x564dc0e570e0 .event edge, v0x564dc0e7efc0_0, v0x564dc0e7f080_0, v0x564dc0e7eee0_0;
    .scope S_0x564dc0e560f0;
T_0 ;
Ewait_0 .event/or E_0x564dc0e570e0, E_0x0;
    %wait Ewait_0;
    %load/vec4 v0x564dc0e7efc0_0;
    %pad/s 16;
    %store/vec4 v0x564dc0e2bbd0_0, 0, 16;
    %load/vec4 v0x564dc0e7f080_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %load/vec4 v0x564dc0e7eee0_0;
    %store/vec4 v0x564dc0e7f160_0, 0, 16;
    %jmp T_0.4;
T_0.0 ;
    %load/vec4 v0x564dc0e7eee0_0;
    %load/vec4 v0x564dc0e2bbd0_0;
    %add;
    %store/vec4 v0x564dc0e7f160_0, 0, 16;
    %jmp T_0.4;
T_0.1 ;
    %load/vec4 v0x564dc0e7eee0_0;
    %load/vec4 v0x564dc0e2bbd0_0;
    %sub;
    %store/vec4 v0x564dc0e7f160_0, 0, 16;
    %jmp T_0.4;
T_0.2 ;
    %load/vec4 v0x564dc0e7eee0_0;
    %store/vec4 v0x564dc0e7f160_0, 0, 16;
    %jmp T_0.4;
T_0.4 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x564dc0e530f0;
T_1 ;
    %vpi_call/w 4 19 "$display", "---------------------------------------" {0 0 0};
    %vpi_call/w 4 20 "$display", "   Mission 1 Verification Start" {0 0 0};
    %vpi_call/w 4 21 "$display", "---------------------------------------" {0 0 0};
